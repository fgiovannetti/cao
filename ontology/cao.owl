    <!-- http://iflastandards.info/ns/fr/frbr/frbroo/F2 -->

    <owl:Class rdf:about="http://iflastandards.info/ns/fr/frbr/frbroo/F2">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
        <rdfs:comment xml:lang="en">This class comprises the intellectual or artistic realisations of works in the form of identifiable immaterial objects, such as texts, poems, jokes, musical or choreographic notations, movement pattern, sound pattern, images, multimedia objects, or any combination of such forms that have objectively recognisable structures.</rdfs:comment>
        <rdfs:isDefinedBy>http://iflastandards.info/ns/fr/frbr/frbroo/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">F2 Expression</rdfs:label>
    </owl:Class>
    


    <!-- http://iflastandards.info/ns/fr/frbr/frbroo/F23 -->

    <owl:Class rdf:about="http://iflastandards.info/ns/fr/frbr/frbroo/F23">
        <rdfs:subClassOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F2"/>
        <rdfs:comment xml:lang="en">This class comprises parts of Expressions and these parts are not Self-contained Expressions themselves.</rdfs:comment>
        <rdfs:isDefinedBy>http://iflastandards.info/ns/fr/frbr/frbroo/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">F23 Expression Fragment</rdfs:label>
